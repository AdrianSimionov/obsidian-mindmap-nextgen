{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2020"
    ],
    "paths": {
      "obsidian": ["src/types/obsidian.d.ts"],
      "d3-svg-to-png": ["src/types/d3SvgToPng.d.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "src/types",
  ]
}
